<?xml version="1.0"?>

<ivy-module
	version="2.0"
	xmlns:m2="http://ant.apache.org/ivy/maven"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
>
	<info module="${ivy.module.name}" organisation="com.liferay">
		<extends extendType="configurations,description,info" location="${sdk.dir}/ivy.xml" module="com.liferay.sdk" organisation="com.liferay" revision="latest.integration" />
	</info>

	<dependencies defaultconf="default">
		<dependency name="aws-java-sdk" org="com.amazonaws" rev="1.7.9" />
		<dependency name="bcprov-jdk16" org="org.bouncycastle" rev="1.46" />
		<dependency name="com.liferay.petra.json.web.service.client" org="com.liferay" rev="1.0.1" />
		<dependency name="httpclient" org="org.apache.httpcomponents" rev="4.3.3" />
		<dependency name="httpcore" org="org.apache.httpcomponents" rev="4.3.2" />
		<dependency name="jcl-over-slf4j" org="org.slf4j" rev="1.7.5" />
		<dependency name="joda-time" org="joda-time" rev="2.3" />
		<dependency name="json" org="org.json" rev="20140107" />
		<dependency name="jzlib" org="com.jcraft" rev="1.1.3" />
		<dependency name="log4j" org="log4j" rev="1.2.17" />
		<dependency name="net.sf.jargs" org="com.liferay" rev="1.0" />
		<dependency name="slf4j-api" org="org.slf4j" rev="1.7.5" />
		<dependency name="slf4j-log4j12" org="org.slf4j" rev="1.7.5" />
		<dependency name="sshj" org="net.schmizz" rev="0.9.0" />
	</dependencies>
</ivy-module>