<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.0.0+</version>
	</compatibility>
	<theme id="so" name="Social Office">
		<template-extension>vm</template-extension>
		<color-scheme id="01" name="Classic">
			<default-cs>true</default-cs>
			<css-class>so-classic</css-class>
		</color-scheme>
		<color-scheme id="02" name="Strata">
			<css-class>so-strata</css-class>
		</color-scheme>
	</theme>
</look-and-feel>